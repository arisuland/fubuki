{
  "name": "@arisu/gh-to-youtrack",
  "description": "Tiny webhook to post GitHub issues towards our YouTrack issue tracker.",
  "version": "1.0.0",
  "main": "build/main.js",
  "author": "Noel <cutie@floofy.dev>",
  "license": "GPL-3.0",
