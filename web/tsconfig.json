{
  "extends": "@augu/tsconfig/dom.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "typeRoots": ["src/@types", "./node_modules/@types"],
    "types": [
      "node",
      "@nuxt/types",
      "@nuxtjs/apollo/types/nuxt",
      "@nuxtjs/i18n/types/vue",
      "@nuxtjs/pwa/dist/pwa",
      "@nuxtjs/sentry/types"
    ],
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"]
}
